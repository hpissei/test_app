<div class="container">
<h1>Users index</h1>
<table class="table">
    <tr><th>First Name</th><th>Details</th><th>Edit Details</th>
    <% if current_user=="" || current_user.admin%>
    <!-- user not signed in
    <%= "#user#{current_user.to_s}" %> 
    -->
      <th>Delete</th>
    <%end %>
    </tr>
    <% @users.each do |user| %>
    <tr> 
       <td><%= user.first_name %></td>
        <td><%= link_to "View Details",user %></td>
        <td><%= link_to "Edit",edit_user_path(user) %></td>
       <% if current_user=="" || current_user.admin %>
        <td><%=  link_to "Delete",user_delete_path(user),method: :delete,data:{confirm:"Are you sure you want to delete this user"} %></td>
        <% end %>
    </tr>
    <% end %>
</table>
<%= will_paginate @users,:class=>"",:style=>"float:right;" %>
<%= link_to "User Home",users_path %>
<%= link_to "User New",user_new_create_url %>

</div>
