<h1>Work Edit</h1>
<%= form_for(@work) do |f| %>
    <div>
    <%= f.label :project_id %>
    <%= f.collection_select(:project_id,Project.all,:id,:project_name) %>
    <div>
    <div>
    <%= f.label :user_id %>
    <%= f.collection_select(:user_id,User.all,:id,:first_name) %>
    </div>
    <div>
        <%= f.label :date_performed %>
        <%= f.date_field(:date_performed) %>
    </div>
    <%= f.submit "Edit Work" %>
<% end %>