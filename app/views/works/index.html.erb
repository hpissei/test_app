<h1>Work index</h1>
<table class="table">
<tr>
    <th>Work id</th>
    <th>Work project</th>
    <th>View Details</th>
    <th>Edit Details</th>
</tr>
<% @works.each do |work| %>

<tr>
    <td><%= work.id %></td>
    <td><%= work.project.project_name %></td>
    <td><%= link_to "View Details",work %></td>
    <td><%= link_to "Edit Details",edit_work_path(work) %></td>
</tr>
<% end %>
</table>

<%= link_to "Work Home",works_path %>
<%= link_to "New Work",works_new_path %>
