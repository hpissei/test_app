<h1>Companies index</h1>

<table class="table">
<tr>
    <th>Company Id</th>
    <th>Company Name</th>
    <th>View Details</th>  

</tr>

<% @company.each do |company| %>
    <tr>   
        <td><%= company.id %> 
        <td><%= company.name %></td>
        <td><%= link_to  "View Details",company %></td>
        <td><%= link_to "Edit",companies_edit_path(company) %></td>
  
    </tr>
<% end %>
</table><br>
<%= link_to "Company Home",companies_path %>
<%= link_to "Company New",companies_new_path %>

<aside>
    <%= render partial:"sidebar" %> 
</aside>

