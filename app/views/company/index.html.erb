<h1>Companies index</h1>

<table class="table">
<tr>
    <th>Company Id</th>
    <th>Company Name</th>
    <th>View Details</th>  
</tr>

<% @company.each do |company| %>
    <tr>   
        <td><%= company.id %> 
        <td><%= company.name %></td>
        <td><%= link_to  "View Details",company %></td>
    </tr>
<% end %>
</table><br>
<%= link_to "Company Home",company_index_path %>

